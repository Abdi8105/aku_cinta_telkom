{
    "version": 2,
    "framework": null,
    "buildCommand": "",
    "outputDirectory": "public",
    "installCommand": "composer install",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.8.4"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/(css|js|images|fonts|favicon.ico)(.*)",
            "dest": "/public/$1$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}